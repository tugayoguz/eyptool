	 SON_ARSIV_DURUM  INTEGER , 
	 SON_ILGI_DURUM  INTEGER, 
	 SAKLAMA_PLAN_NO  INTEGER, 
	 TASFIYE_TARIHI  TIMESTAMP (6), 
	 TASFIYE_ISLEMI  VARCHAR(2 ), 
	 TASFIYE_EDILDI  VARCHAR(1 ), 
	 IADE  INTEGER , 
	 BLSM_NODE_ID  INTEGER, 
