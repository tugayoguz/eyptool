CREATE TABLE users (
  id         INTEGER PRIMARY KEY,
  name VARCHAR(30),
  email  VARCHAR(50)
);


  CREATE TABLE T_EVR_EVRAK
   (EVRAK_NO INTEGER PRIMARY KEY, 
	ICERIK_DOSYA_NO INTEGER, 
	EVRAK_SAYI VARCHAR(500), 
	EVRAK_TARIHI TIMESTAMP (6), 
	KURUM_NO INTEGER, 
	KONU_NO INTEGER, 
	GELIS_SEKLI VARCHAR(2), 
	GIZLILIK VARCHAR(2), 
	SURELI VARCHAR(1), 
	SURE_BITIS_TARIHI TIMESTAMP (6), 
	IVEDILIK VARCHAR(2), 
	YAZISMA_TIPI VARCHAR(3), 
	EVRAK_TURU VARCHAR(10), 
	BIRIM_NO INTEGER, 
	YER VARCHAR(4000), 
	ACIKLAMA VARCHAR(4000), 
	ILGI VARCHAR(4000), 
	EK VARCHAR(500), 
	LAST_UPDATE_DATE  TIMESTAMP (6), 
	KURUM_EVRAK_NO  VARCHAR(30), 
	KURUM_EVRAK_TARIHI  TIMESTAMP (6), 
	KONU_TEXT  VARCHAR(4000), 
	KURUM_DEFTER_BIRIM_NO  INTEGER, 
	DAGITIM_ACIKLAMA  VARCHAR(4000), 
	ONAYDAN_SONRA_DAGIT  VARCHAR(1), 
	EVRAK_CINSI  VARCHAR(2), 
	BOLUM_NO  INTEGER, 
	KISISEL_BILGI  VARCHAR(1), 
	BILGI_EDINME  VARCHAR(1), 
	URETILDIGI_EVRAK_NO  INTEGER, 
	URETICI  VARCHAR(300), 
	YAZAR  VARCHAR(300), 
	BELGE_REFERANS_NO  VARCHAR(500), 
	TELIF_HAKLARI_KANUN_KAPSAMI  VARCHAR(1), 
	GUVENLI_ELEKTRONIK_IMZA  VARCHAR(1), 
	BELGE_STAT_KAZANMAYAN_DOKUMAN  VARCHAR(1), 
	ELEKTRONIK_SERTIFIKA_SAGLAYICI  VARCHAR(500), 
	ZAMAN_DAMGASI  TIMESTAMP (6), 
	SIFRELEME_ALGORITMASI  VARCHAR(500), 
	DIGER_SAYISAL_ISARETLER  VARCHAR(500), 
	FIZIKSEL_DOSYA  VARCHAR(1), 
	SAKLAMA_YERI  VARCHAR(4000), 
	REPEATFORALLPAGES  VARCHAR(100), 
	PAGES_TO_BE_STAMPED  VARCHAR(50 ));
	
	 CREATE TABLE T_EVR_EVRAK_HAREKET  
   ( HAREKET_NO  INTEGER PRIMARY KEY, 
	 ONCEKI_HAREKET_NO  INTEGER, 
	 EVRAK_NO  INTEGER, 
	 EVRAK_TIPI  VARCHAR(5 ), 
	 EVRAK_DURUMU  VARCHAR(2 ), 
	 HAREKET_TURU  VARCHAR(2 ), 
	 HAREKET_TARIHI  TIMESTAMP (6), 
	 HAREKET_NOT  VARCHAR(700 ), 
	 BIRIM_NO  INTEGER, 
	 BIRIM_EVRAK_NO  VARCHAR(30 ), 
	 BIRIM_EVRAK_TARIHI  TIMESTAMP (6), 
	 DOSYA_NO  INTEGER, 
	 ISLEM_DURUMU  VARCHAR(2 ), 
	 ISLEM_TARIHI  TIMESTAMP (6), 
	 ILGILI_KULLANICI_NO  INTEGER, 
	 ILGILI_BIRIM_NO  INTEGER, 
	 ILGILI_GOREV_NO  INTEGER, 
	 ISLEM_YAPAN_KULLANICI_NO  INTEGER, 
	 ISLEM_YAPAN_BIRIM_NO  INTEGER, 
	 ISLEM_YAPAN_GOREV_NO  INTEGER, 
	 SURE_BITIS_TARIHI  TIMESTAMP (6), 
	 TASKINSTANCE_ID  INTEGER, 
	 LAST_UPDATE_DATE  TIMESTAMP (6), 
	 DEFTER_BIRIM_NO  INTEGER, 
